{
    "name": "zalas/phpunit-doubles",
    "description": "Initialises test doubles in PHPUnit test cases for you",
    "type": "library",
    "require": {
        "php": "^7.4 || ~8.0.0",
        "phpunit/phpunit": "^9.0",
        "phpdocumentor/reflection-docblock": "^5.2"
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Zalas\\PHPUnit\\Doubles\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zalas\\PHPUnit\\Doubles\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub Zalas",
            "email": "jakub@zalas.pl"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.4.x-dev"
        }
    }
}
